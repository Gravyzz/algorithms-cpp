#include <iostream>
#include <vector>

using namespace std;

const int kINF = 1e9;


void readMatrix(vector<vector<int>>& matrix, int N) {
    for (int i = 0; i < N; ++i) {
        for (int j = 0; j < N; ++j) {
            cin >> matrix[i][j];
        }
    }
}


vector<int> deksrt(const vector<vector<int>>& matrix, int S) {
    int N = matrix.size();

    vector<int> d(N, kINF);  
    vector<int> used(N, 0); 

    d[S] = 0;

    for (int i = 0; i < N; ++i) {
        int v = -1;

        for (int j = 0; j < N; ++j) {
            if (!used[j] && (v == -1 || d[j] < d[v])) {
                v = j;
            }
        }

        if (v == -1 || d[v] == kINF) break;

        used[v] = 1;

        for (int to = 0; to < N; ++to) {
            if (matrix[v][to] != -1) {
                if (d[v] + matrix[v][to] < d[to]) {
                    d[to] = d[v] + matrix[v][to];
                }
            }
        }
    }

    return d;
}

int main() {
    int N, S, F;
    cin >> N >> S >> F;
    S--;
    F--;

    vector<vector<int>> matrix(N, vector<int>(N));
    readMatrix(matrix, N);

    vector<int> dist = deksrt(matrix, S);

    cout << dist[F] << endl;

    return 0;
}
